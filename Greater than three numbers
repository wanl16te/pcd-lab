#include <stdio.h>
int main ()
{
  int a,b,c;
  
  printf("please enter any three numbers\n");
  scanf("%d%d%d",&a,&b,&c);
  if (a>b&&a>c)
  printf("the greater number is:%d",a);
  else if (b>c)
  printf("the greater number is:%d",b);
  else
  printf("the greater number is:%d",c);
     
return 0;
}
     
